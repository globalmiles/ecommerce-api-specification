<?xml version="1.0" encoding="UTF-8"?>
<application xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://wadl.dev.java.net/2009/02" xmlns:tns="default/schemas">
  <doc title="Global Miles Ecommerce API" xml:lang="en">API for Ecommerce Partners. This document includes Cart, Discount, Payment, Order and Return related endpoints provided by Global Miles.</doc>
  <grammars>
    <xs:schema xmlns:tns="default/schemas" targetNamespace="default/schemas" xmlns:xs="http://www.w3.org/2001/XMLSchema">
      <xs:element name="CompleteReturnRequest" type="tns:CompleteReturnRequest" />
      <xs:element name="StartReturnResponse" type="tns:StartReturnResponse" />
      <xs:element name="StartReturnRequest" type="tns:StartReturnRequest" />
      <xs:element name="DiscountType" type="tns:DiscountType" />
      <xs:element name="StartReturnResult" type="tns:StartReturnResult" />
      <xs:element name="CartResponse" type="tns:CartResponse" />
      <xs:element name="ListMilePaymentResponse" type="tns:ListMilePaymentResponse" />
      <xs:element name="OrderResult" type="tns:OrderResult" />
      <xs:element name="OrderResponse" type="tns:OrderResponse" />
      <xs:element name="OrderItem" type="tns:OrderItem" />
      <xs:element name="PaymentItem" type="tns:PaymentItem" />
      <xs:element name="OAuthResponse" type="tns:OAuthResponse" />
      <xs:element name="OAuthRequest" type="tns:OAuthRequest" />
      <xs:element name="Order" type="tns:Order" />
      <xs:element name="PaymentType" type="tns:PaymentType" />
      <xs:element name="CartResult" type="tns:CartResult" />
      <xs:element name="CartItem" type="tns:CartItem" />
      <xs:element name="Cart" type="tns:Cart" />
      <xs:element name="CartItemDiscount" type="tns:CartItemDiscount" />
      <xs:element name="DiscountItem" type="tns:DiscountItem" />
      <xs:element name="OrderRequest" type="tns:OrderRequest" />
      <xs:element name="CartRequest" type="tns:CartRequest" />
      <xs:element name="Response" type="tns:Response" />
      <xs:element name="Amount" type="tns:Amount" />
      <xs:element name="StartMilePaymentRequest" type="tns:StartMilePaymentRequest" />
      <xs:element name="StartMilePaymentResponse" type="tns:StartMilePaymentResponse" />
      <xs:element name="StartMilePaymentResult" type="tns:StartMilePaymentResult" />
      <xs:element name="CompleteMilePaymentRequest" type="tns:CompleteMilePaymentRequest" />
      <xs:element name="CancelMilePaymentRequest" type="tns:CancelMilePaymentRequest" />
      <xs:element name="RefundMilePaymentRequest" type="tns:RefundMilePaymentRequest" />
      <xs:element name="PaymentStatus" type="tns:PaymentStatus" />
      <xs:element name="Payment" type="tns:Payment" />
      <xs:element name="CancelReturnRequest" type="tns:CancelReturnRequest" />
      <xs:element name="CartDiscount" type="tns:CartDiscount" />
      <xs:complexType name="CompleteReturnRequest">
        <xs:annotation>
          <xs:documentation>Request of return for complete action.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="store_code">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>An identifier for online store.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="return_provision_token">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>A token represents a return object for provisioning.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="StartReturnResponse">
        <xs:annotation>
          <xs:documentation>Response object of return for start action.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="false">
          <xs:extension base="tns:Response">
            <xs:sequence>
              <xs:element minOccurs="1" name="result" type="tns:StartReturnResult">
                <xs:annotation>
                  <xs:documentation>Result object of the response.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="StartReturnRequest">
        <xs:annotation>
          <xs:documentation>Request of return for start action.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="store_code">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>An identifier for online store.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="transaction_id">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The ID of the transaction that represents the order.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="amount" type="tns:Amount">
            <xs:annotation>
              <xs:documentation>An amount of the return.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="DiscountType">
        <xs:annotation>
          <xs:documentation>A type of discount.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:enumeration value="loyalty_discount">
            <xs:annotation>
              <xs:documentation>This type indicates a discount by Global Miles.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="other">
            <xs:annotation>
              <xs:documentation>Other type of discounts.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="StartReturnResult">
        <xs:annotation>
          <xs:documentation>Result object of return for start action.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="return_provision_token">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>A token represents a return object for provisioning.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="CartResponse">
        <xs:annotation>
          <xs:documentation>Response object of the cart.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="false">
          <xs:extension base="tns:Response">
            <xs:sequence>
              <xs:element minOccurs="0" name="result" type="tns:CartResult">
                <xs:annotation>
                  <xs:documentation>Result object of the response.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="ListMilePaymentResponse">
        <xs:annotation>
          <xs:documentation>Response object of payment for list action.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="false">
          <xs:extension base="tns:Response">
            <xs:sequence>
              <xs:element minOccurs="0" maxOccurs="unbounded" name="result" type="tns:Payment">
                <xs:annotation>
                  <xs:documentation>Result object of the response.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="OrderResult">
        <xs:annotation>
          <xs:documentation>Result object of the response.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="transaction_token">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>A token represents an order object from Global Miles system.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="OrderResponse">
        <xs:annotation>
          <xs:documentation>Response object of order.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="false">
          <xs:extension base="tns:Response">
            <xs:sequence>
              <xs:element minOccurs="1" name="result" type="tns:OrderResult">
                <xs:annotation>
                  <xs:documentation>Result object of the response.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="OrderItem">
        <xs:annotation>
          <xs:documentation>Representing an order item.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="id">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>An identifier to reference this product.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="quantity">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The quantity of the item.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:double" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="price">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The price of the item.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:double" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="PaymentItem">
        <xs:annotation>
          <xs:documentation>Representing a payment for order.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="type" type="tns:PaymentType">
            <xs:annotation>
              <xs:documentation>A type of payment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" name="amount">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Amount of current payment.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:double" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="0" name="payment_provision_token">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>A token represents a payment object for provisioning.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="OAuthResponse">
        <xs:annotation>
          <xs:documentation>Representing an OAuth response.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="access_token">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>An access token from the authorization server.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="token_type">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The type of token this is, typically just the string "bearer".</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="created_at">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The time in unix time format when the access token was created.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="OAuthRequest">
        <xs:annotation>
          <xs:documentation>Representing an OAuth request.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="client_id">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>ID of the client which is provided by Global Miles.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="client_secret">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Secret of the client which is provided by Global Miles.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" default="client_credentials" name="grant_type">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Type of the OAuth flows. We support only one type of token: client_credentials</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="Order">
        <xs:annotation>
          <xs:documentation>Representing an order.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="subtotal">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Total value of all order items without tax.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:double" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="tax">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The amount of tax.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:double" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="total">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Total amount of order.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:double" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="currency">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>ISO-4217 3-letter currency code.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="items" type="tns:OrderItem">
            <xs:annotation>
              <xs:documentation>An array of order items.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="discounts" type="tns:DiscountItem">
            <xs:annotation>
              <xs:documentation>An array of discount items.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="payments" type="tns:PaymentItem">
            <xs:annotation>
              <xs:documentation>An array of payment items.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="PaymentType">
        <xs:annotation>
          <xs:documentation>A type of payment.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:enumeration value="credit_card">
            <xs:annotation>
              <xs:documentation>Credit Card</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="debit_card">
            <xs:annotation>
              <xs:documentation>Debit Card</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="bank_transfer">
            <xs:annotation>
              <xs:documentation>Bank Transfers / Electronic Funds Transfer (EFT)</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="cash">
            <xs:annotation>
              <xs:documentation>Cash</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="digital_wallet">
            <xs:annotation>
              <xs:documentation>Digital Wallet</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="mobile_payment">
            <xs:annotation>
              <xs:documentation>Mobile Payment</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="gift_card">
            <xs:annotation>
              <xs:documentation>Gitf Card</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="mile_payment">
            <xs:annotation>
              <xs:documentation>Pay with miles provided by Global Miles</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="other">
            <xs:annotation>
              <xs:documentation>Other</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="CartResult">
        <xs:annotation>
          <xs:documentation>Result object of the response.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="0" name="available_amount" type="tns:Amount">
            <xs:annotation>
              <xs:documentation>The available amount of money for payment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="cart_discount" type="tns:CartDiscount">
            <xs:annotation>
              <xs:documentation>A discount object for cart.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="CartItem">
        <xs:annotation>
          <xs:documentation>Representing a shopping cart item.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="id">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>An identifier to reference this product. Universal product code.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="quantity">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The quantity of the item.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:double" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="price">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The price of the item.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:double" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="Cart">
        <xs:annotation>
          <xs:documentation>Representing a shopping cart.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="currency">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>ISO-4217 3-letter currency code.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="subtotal">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Total value of all cart items without tax.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:double" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="tax">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The amount of tax.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:double" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="total">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Total amount of cart.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:double" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="items" type="tns:CartItem">
            <xs:annotation>
              <xs:documentation>An array of cart items.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="CartItemDiscount">
        <xs:annotation>
          <xs:documentation>Representing a discount for shopping cart item.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="id">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>An identifier to reference this product.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="discount">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Amount of the current discount.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:double" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="DiscountItem">
        <xs:annotation>
          <xs:documentation>Representing a discount for order.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="type" type="tns:DiscountType">
            <xs:annotation>
              <xs:documentation>Type of the discount.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" name="amount">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Amount of the current discount.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:double" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="0" name="discount_provision_token">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>A token represents a discount object for provisioning.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="OrderRequest">
        <xs:annotation>
          <xs:documentation>Request of order object.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="store_code">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>An identifier for online store.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="user_token">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>A token that is representing a Global Miles user for the current session.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="transaction_id">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The ID of the transaction that represents the order.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="order" type="tns:Order">
            <xs:annotation>
              <xs:documentation>A complex object for order.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" default="0" name="shift_allowance">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>It specifies how many days later the miles should be given to the user.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:integer" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="CartRequest">
        <xs:annotation>
          <xs:documentation>Request of the card object.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="user_token">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>A token that is representing a Global Miles user for the current session.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="store_code">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>An identifier for online store.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="cart" type="tns:Cart">
            <xs:annotation>
              <xs:documentation>A complex object for cart.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="Response">
        <xs:annotation>
          <xs:documentation>A basic object of the response.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="status">
            <xs:simpleType>
              <xs:annotation>
